{
  "name": "chrome-extension-boilerplate-react-vite",
  "version": "0.0.1",
  "description": "chrome extension boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "build": "vite build",
    "build:v2": "cross-env MANIFEST_VERSION=2 run-s build",
    "build:all": "run-p build build:v2",
    "clear": "rimraf dist/*",
    "clear:v2": "rimraf dist/v2/",
    "clear:v3": "rimraf dist/v3/",
    "watch": "vite build --watch --mode development --minify false",
    "watch:v2": "cross-env MANIFEST_VERSION=2 run-s watch",
    "watch:all": "run-p watch watch:v2",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/v2",
    "serve:chrome": "web-ext run -t chromium --start-url \"http://google.com/\" --source-dir ./dist/v3",
    "serve:all": "run-p serve:firefox serve:chrome",
    "pack:crx": "cross-env-shell crx pack dist/v3 -o $npm_package_name-$npm_package_version.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ cross-env-shell web-ext build --source-dir ./dist/v2 --filename $npm_package_name-$npm_package_version.xpi --overwrite-dest",
    "pack:all": "run-p pack:crx pack:xpi"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.9.11",
    "@mozilla/readability": "^0.4.4",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "5.0.0-alpha.120",
    "@mui/material": "^5.11.9",
    "@samrum/vite-plugin-web-extension": "^4.1.0",
    "@tanstack/react-query": "^4.24.6",
    "@tanstack/react-query-devtools": "^4.24.6",
    "async-mutex": "^0.4.0",
    "date-fns": "^2.29.3",
    "draft-js": "^0.11.7",
    "i18next": "^22.4.10",
    "jotai": "^2.1.0",
    "jsonld": "^8.1.1",
    "lodash": "^4.17.21",
    "masonic": "^3.7.0",
    "qs": "^6.11.2",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.1.5",
    "use-resize-observer": "^9.1.0",
    "webextension-polyfill": "^0.10.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@mdx-js/react": "^2.3.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "^0.0.233",
    "@types/draft-js": "^0.11.10",
    "@types/jest": "29.0.3",
    "@types/node": "18.15.11",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.11",
    "@types/react-draft-wysiwyg": "^1.13.4",
    "@types/webextension-polyfill": "^0.10.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "cypress": "^12.6.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-storybook": "^0.6.10",
    "fs-extra": "11.1.0",
    "husky": "^8.0.3",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "rollup": "2.79.1",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "vite": "^4.2.0",
    "webpack": "^5.75.0",
    "ws": "8.13.0"
  }
}